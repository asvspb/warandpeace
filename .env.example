# --- Telegram ---
# Токен вашего Telegram-бота. Получается у @BotFather в Telegram.
TELEGRAM_BOT_TOKEN=
# ID вашего Telegram-канала (публичного или частного). Для публичного: @username, для частного: -100...
TELEGRAM_CHANNEL_ID=
# Ваш личный Telegram User ID для получения уведомлений об ошибках.
TELEGRAM_ADMIN_ID=

# --- Telegram Network Settings (optional) ---
# Таймауты для HTTP-клиента Telegram (в секундах)
# TG_READ_TIMEOUT=30
# TG_WRITE_TIMEOUT=30
# TG_CONNECT_TIMEOUT=10
# TG_POOL_TIMEOUT=5
# Время жизни кэша для информации о канале (в секундах)
TELEGRAM_CACHE_TTL_SEC=3600

# --- AI Models ---
# Глобальные переключатели и настройки
GEMINI_ENABLED=true
MISTRAL_ENABLED=true
LLM_PRIMARY=gemini # gemini или mistral
LLM_TIMEOUT_SEC=30
LLM_MAX_TOKENS=512

# Настройки Gemini
# Основной ключ Google API.
GOOGLE_API_KEY=
# Дополнительные ключи (опционально, для распределения нагрузки).
GOOGLE_API_KEY_1=
# Имя модели Gemini для использования.
GEMINI_MODEL="gemini-2.0-flash"

# Настройки Mistral
# Ключ API для Mistral AI
MISTRAL_API_KEY=
# Имя модели Mistral для использования
MISTRAL_MODEL_NAME="mistral-large-latest"